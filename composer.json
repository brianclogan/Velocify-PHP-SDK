{
    "name": "darkgoldblade01/velocify-php-sdk",
    "description": "Velocify PHP API - an easy to use version of the PHP Velocify API using GuzzleHTTP.",
    "keywords": [
        "Velocify",
        "php",
        "sdk"
    ],
    "homepage": "http://github.com/darkgoldblade01",
    "license": "",
    "authors": [
        {
            "name": "Brian Logan",
            "homepage": "https://github.com/darkgoldblade01"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "~6.0",
        "nesbot/carbon": "^1.22",
        "suramon/simple-xml-reader": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "victorjonsson/markdowndocs": "^1.3",
        "wanfeiyy/dd": "^1.0"
    },
    "autoload": {
        "psr-4": { "darkgoldblade01\\Velocify\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "darkgoldblade01\\Velocify\\" : "src/" }
    }
}
